{
  "name": "excalibur-client",
  "version": "0.1.0",
  "main": "public/electron.js",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^5.2.0",
    "fs": "^0.0.1-security",
    "msr": "^1.3.4",
    "prop-types": "^15.7.2",
    "ps-list": "^7.2.0",
    "python-shell": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "^3.9.6"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "start-with-cam": "concurrently \"tail -f dev/log.txt\" \"tsc ./src/electron.ts --outDir ./public --watch\" \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && open node_modules/electron/dist/Electron.app --args $PWD\"",
    "start": "concurrently \"tsc ./src/electron.ts --outDir ./public --watch\" \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "yarn react-build && electron-builder",
    "release": "yarn react-build && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-reloader": "^1.0.1",
    "wait-on": "^5.1.0"
  }
}
