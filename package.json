{
  "name": "excalibur-client-r",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./dist/electron.js",
  "scripts": {
    "electron:dev": "tsc ./src/electron.ts --outDir ./dist",
    "react:dev": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" NODE_ENV=development webpack-dev-server --hot --config webpack.config.ts",
    "waiton:dev": "wait-on http://localhost:3000 && electron .",
    "start": "yarn electron:dev && concurrently \"yarn react:dev\"  \"yarn waiton:dev\""
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@types/babel__core": "^7.1.9",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "^3.11.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.0",
    "electron-is-dev": "^1.2.0",
    "html-webpack-plugin": "^4.4.1",
    "react-hot-loader": "^4.12.21",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.3"
  },
  "dependencies": {
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.3.0",
    "electron-reload": "^1.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts": "^0.2.2",
    "wait-on": "^5.2.0"
  }
}
